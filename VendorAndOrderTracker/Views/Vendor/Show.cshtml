@{
  Layout = "_Layout";
}

@using VendorAndOrderTracker.Models;

<h3>Here are all the orders for this Vendor:</h3>

<ol>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendor/@Model["vendor"].Id/orders/@order.Id'>@order.OrderName</a></li>
  }
</ol>
<p><a href='/vendor/@Model["vendor"].Id/orders/new'>Add a new Order</a></p>
<p><a href='/vendor'>Return to Vendor list</a></p>
<p><a href='/'>Return to Main Page</a></p>